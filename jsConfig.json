{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"],
      "@stores/*": ["resources/js/stores/*"],
      "@services/*": ["resources/js/services/*"],
      "@components/*": ["resources/js/components/*"],
      "@plugins/*": ["resources/js/plugins/*"],
      "@assets/*": ["resources/js/assets/*"],
      "@views/*": ["resources/js/views/*"],
    },
    "ignoreDeprecations": "6.0"
  },
  "exclude": [
    "node_modules",
    "public"
  ]
}